[core]
	autocrlf = false
	filemode = false
    quotepath = false
	deltaBaseCacheLimit = 1G
[color]
    status = auto
    diff = auto
    branch = auto
    interactive = auto
[user]
	email = 
	name = 
[push]
    default = matching
[http]
#   proxy = socks5://127.0.0.1:8119
#   proxy = http://127.0.0.1:8103
[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    co = checkout
    ci = commit
    st = status
    br = branch
